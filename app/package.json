{
  "name": "@crosshare/web",
  "version": "0.0.0",
  "author": "Mike D",
  "private": true,
  "type": "module",
  "browserslist": [
    ">0.2%",
    "last 2 versions",
    "unreleased versions",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "scripts": {
    "supportedBrowsers": "echo \"export const browserRegex = $(browserslist-useragent-regexp --allowHigherVersions);\" > lib/supportedBrowsers.ts",
    "lint": "npx eslint components lib pages reducers scripts && npx stylelint \"**/*.css\"",
    "knip": "npx knip",
    "format": "npx prettier \"../**/*\\.(ts(x)?|module\\.css)\" --write",
    "checkFormat": "npx prettier \"../**/*\\.(ts(x)?|module\\.css)\" --check",
    "clean": "rimraf nextjs/",
    "dev": "GOOGLE_APPLICATION_CREDENTIALS=../serviceAccountKey.json next dev",
    "analyze": "ANALYZE=true next build",
    "build": "next build",
    "start": "next start",
    "extract": "npx lingui extract",
    "compileI18n": "npx lingui compile",
    "predeploy": "tsc && pnpm clean && next build",
    "prodDeploy": "npx firebase deploy --only hosting:prod",
    "stagingDeploy": "npx firebase deploy --only hosting:staging",
    "test": "NODE_OPTIONS='--experimental-vm-modules' npx jest --watch",
    "emulatorAndTest": "firebase emulators:exec --only firestore --project demo-crosshare 'NODE_OPTIONS='--experimental-vm-modules'  npx jest --watch'",
    "emulate": "NEXT_PUBLIC_USE_EMULATORS=1 METADATA_SERVER_DETECTION=none GCE_METADATA_HOST=0.0.0.0 FIRESTORE_EMULATOR_HOST=\"127.0.0.1:8080\" GCLOUD_PROJECT=\"demo-crosshare\" firebase emulators:exec --import=./emulator-data --only auth,firestore,storage --project demo-crosshare --ui 'npx next dev'"
  },
  "engines": {
    "node": "20"
  },
